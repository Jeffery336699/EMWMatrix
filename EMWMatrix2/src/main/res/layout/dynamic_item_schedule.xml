<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/tv_dynamicschedule_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/index_ico_rijihua"
        android:drawablePadding="5dp"
        android:gravity="center"
        android:textColor="@color/dynamic_title_text"
        android:textSize="14sp"
        android:text="2016-1-1" />

    <TextView
        android:id="@+id/tv_dynamicschedule_starttime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="28dp"
        android:drawablePadding="10dp"
        android:text="2015-12-25"
        android:textColor="@color/dynamicschedule_date_text"
        android:textSize="14sp" />
    	<!-- android:drawableLeft="@drawable/home_schedule_img_start" -->
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="14dp" />
    	<!-- android:src="@drawable/home_schedule_img_line" -->
    <TextView
        android:id="@+id/tv_dynamicschedule_endtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:drawablePadding="10dp"
        android:text="2015-12-26"
        android:textColor="@color/dynamicschedule_date_text"
        android:textSize="14sp" 
        android:visibility="gone"/>
    	<!-- android:drawableLeft="@drawable/home_schedule_img_end" -->

</LinearLayout>