<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <item name="android:windowIsTranslucent">true</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="ChatTheme" parent="AppBaseTheme">
        <item name="android:windowIsTranslucent">true</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/white</item>
    </style>

    <style name="AppThemeLogin" parent="AppBaseTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/start_bg</item>
    </style>

    <style name="AppThemeMain" parent="AppBaseTheme">
        <item name="android:windowIsTranslucent">false</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="AppThemeForm" parent="AppBaseTheme">
        <item name="android:windowIsTranslucent">true</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/white</item>
    </style>

    <declare-styleable name="FlowLayout">
        <attr name="android:gravity" />
    </declare-styleable>
    <declare-styleable name="FlowLayout_Layout">
        <attr name="android:layout_gravity" />
    </declare-styleable>

    <style name="RadioButton">
        <item name="android:textColor">@drawable/button_text_color</item>
        <item name="android:textSize">13sp</item>
        <item name="android:minHeight">30dp</item>
        <item name="android:minWidth">58dp</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>

    <style name="loading_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/loading_dialog_bg</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

    <!-- 人员选择TextView -->
    <style name="tv_select_person">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
    </style>
    <!-- 表单选择TextView -->
    <style name="tv_sales_chart">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue_1</item>
        <item name="android:textSize">14sp</item>
    </style>

    <!-- 日程 -->
    <style name="dateStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">12sp</item>
    </style>

    <!-- 颜色选择 -->
    <style name="CustomTheme" parent="android:Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <!-- 通讯录 -->
    <style name="ll_input_content">
        <item name="android:layout_marginTop">0dip</item>
        <item name="android:paddingRight">@dimen/all_content_leftright</item>
        <item name="android:paddingLeft">@dimen/all_content_leftright</item>
        <item name="android:background">@drawable/edittext</item>
    </style>
    s

    <style name="SwitchButtonMD">
        <item name="kswThumbDrawable">@drawable/ksw_md_thumb</item>
        <item name="kswBackColor">@color/ksw_md_back_color</item>
        <item name="kswBackMeasureRatio">1.45</item>
        <item name="kswThumbMarginLeft">-16dp</item>
        <item name="kswThumbMarginTop">-14dp</item>
        <item name="kswThumbMarginRight">-16dp</item>
        <item name="kswThumbMarginBottom">-14dp</item>
        <item name="kswThumbWidth">@dimen/ksw_md_thumb_ripple_size</item>
        <item name="kswThumbHeight">@dimen/ksw_md_thumb_ripple_size</item>
    </style>

    <style name="SwitchButtonStyle">
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
    </style>

    <style name="SwitchButtonStyleMD" parent="SwitchButtonMD">
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
    </style>

    <style name="MaterialDialogSheet" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowAnimationStyle">@style/MaterialDialogSheetAnimation</item>
    </style>

    <style name="MaterialDialogSheetAnimation">
        <item name="android:windowEnterAnimation">@anim/popup_show</item>
        <item name="android:windowExitAnimation">@anim/popup_hide</item>
    </style>

    <!-- we chat -->
    <!-- Navigation Header -->
    <style name="NavPage">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
        <!--  <item name="android:background">@drawable/nav_page</item>
         -->
        <item name="android:background">@color/navpage</item>
    </style>

    <style name="MMTitle">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">19dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="MMTitleButton">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:minWidth">75dp</item>
        <item name="android:background">@null</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="MMFooter">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">54dp</item>
        <item name="android:minHeight">54dp</item>
    </style>

    <style name="MMBody">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:paddingLeft">18dp</item>
        <item name="android:paddingRight">18dp</item>
        <item name="android:paddingTop">26dp</item>
        <item name="android:paddingBottom">18dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:fadingEdge">@null</item>
    </style>

    <style name="MMScroll">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
        <!-- <item name="android:fadingEdge">@null</item> -->
    </style>

    <style name="MMBlock">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MMSplit">
        <item name="android:layout_width">8dp</item>
        <item name="android:layout_height">5dp</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="MMLabelSplit">
        <item name="android:layout_width">10dp</item>
        <item name="android:layout_height">13dp</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="MMEditText">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/BasicTextSize</item>
        <item name="android:singleLine">true</item>
        <item name="android:background">@drawable/mm_edit</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:minHeight">43dp</item>
        <item name="android:textColorHint">@color/lightgrey</item>
    </style>

    <style name="MMLine">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MMLineEditText" parent="@style/MMEditText">
        <item name="android:layout_width">fill_parent</item>
    </style>

    <style name="MMMultiLineEditText" parent="@style/MMLineEditText">
        <item name="android:singleLine">false</item>
        <item name="android:minLines">3</item>
        <item name="android:gravity">top</item>
    </style>

    <style name="MMLineEditPass" parent="@style/MMLineEditText">
        <item name="android:password">true</item>
        <item name="android:inputType">textPassword</item>
    </style>

    <style name="MMButton">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">@color/mm_btn_text</item>
        <item name="android:background">@null</item>
        <item name="android:minHeight">40dp</item>
        <item name="android:minWidth">40dp</item>
    </style>

    <style name="MMCheckBox">
        <item name="android:background">@drawable/mm_checkbox_btn</item>
    </style>

    <style name="MMLineButton" parent="@style/MMButton">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/mm_style_two_btn_text</item>
    </style>

    <style name="MMToolbarButton" parent="@style/MMButton">
        <item name="android:minWidth">45dp</item>
        <item name="android:minHeight">50dp</item>
        <item name="android:textSize">@dimen/SmallerTextSize</item>
        <item name="android:textColor">#2C2C2C</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="MMLineActionButton" parent="@style/MMLineButton">
        <item name="android:background">@drawable/btn_style_one</item>
        <item name="android:textColor">@color/mm_style_one_btn_text</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDy">1.2</item>
        <item name="android:shadowDx">1.2</item>
        <item name="android:shadowRadius">1</item>
    </style>

    <style name="MMBriefText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#707070</item>
        <item name="android:textSize">@dimen/BasicTextSize</item>
        <item name="android:shadowColor">#A0FFFFFF</item>
        <item name="android:shadowDy">1.2</item>
        <item name="android:shadowDx">1.2</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:lineSpacingExtra">2dp</item>
    </style>

    <style name="MMLabel" parent="MMBriefText">
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="MMLineLabel" parent="MMLabel">
        <item name="android:gravity">left</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>

    <style name="MMLogin" parent="MMLabel">
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/LargeTextSize</item>
    </style>

    <style name="MMDivider">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">#B2B2B5</item>
    </style>

    <style name="MMListItem">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:minHeight">60dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingRight">4dp</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="MMFriendListItem" parent="MMListItem">
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">6dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
    </style>

    <style name="MMListCatalog">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">#CED2D7</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#595C61</item>
    </style>
    <!-- weChat -->
    <!-- pay -->
    <style name="MMPreferenceTextLarge">
        <item name="android:textSize">@dimen/LargeTextSize</item>
        <item name="android:textColor">@color/mm_pref_title</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="MMFontPreferenceLarge">
        <item name="android:textAppearance">@style/MMPreferenceTextLarge</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="MMFontPreferenceProfile" parent="MMFontPreferenceLarge">
        <item name="android:textSize">@dimen/TitleTextSize</item>
    </style>

    <style name="TitleText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/BasicTextSize</item>
        <item name="android:textColor">#ff000000</item>
    </style>

    <!-- pay -->

</resources>