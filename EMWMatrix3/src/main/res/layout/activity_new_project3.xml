<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/root_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cm_bg"
    android:orientation="vertical">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:id="@+id/cm_header_bar2"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="@drawable/form_top_meun_white_bg"
        android:gravity="center"
        android:orientation="horizontal">

        <cc.emw.mobile.view.IconTextView
            android:id="@+id/cm_header_btn_left9"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:paddingLeft="12dp"
            android:paddingRight="10dp"
            android:textColor="#101010"
            android:textSize="14dp"
            app:iconCode="ea64" />

        <TextView
            android:id="@+id/cm_header_tv_title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_weight="1"
            android:text=""
            android:textColor="#101010"
            android:textSize="14dp" />
        <cc.emw.mobile.view.IconTextView
            android:id="@+id/cm_header_tv_right7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:background="@drawable/dialog_ok_bg_orange"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="14dp"
            app:iconCode="ea61" />
        <cc.emw.mobile.view.IconTextView
            android:id="@+id/cm_header_tv_right8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:background="@drawable/dialog_ok_bg_red"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="14dp"
            app:iconCode="e92e" />

        <cc.emw.mobile.view.IconTextView
            android:id="@+id/cm_header_tv_right9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="12dp"
            android:background="@drawable/dialog_ok_bg"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="14dp"
            app:iconCode="e931" />
    </LinearLayout>
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:background="@color/white">

        <EditText
            android:id="@+id/et_team_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:background="@color/white"
            android:singleLine="true"
            android:gravity="center_vertical"
            android:hint="请填写团队名称"
            android:textStyle="bold"
            android:textColor="@color/cm_text"
            android:textColorHint="@color/cm_texthint"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/et_team_desc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="16dp"
            android:background="@color/white"
            android:gravity="top"
            android:hint="团队描述"
            android:lines="3"
            android:textColor="@color/cm_text"
            android:textColorHint="@color/cm_texthint"
            android:textSize="14sp" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/cm_divider_bg" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/cm_border_bg"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/rl_team_group"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="?android:attr/selectableItemBackground">

            <cc.emw.mobile.view.IconTextView
                android:id="@+id/team_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:gravity="center"
                android:textColor="#8d8e97"
                android:textSize="16dp"
                app:iconCode="ecd0" />

            <TextView
                android:id="@+id/tv_team_group"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/team_icon"
                android:gravity="center_vertical"
                android:hint="所属圈子"
                android:textColor="@color/cm_text"
                android:textColorHint="@color/cm_texthint"
                android:textSize="14sp" />
            <cc.emw.mobile.view.IconTextView
                android:id="@+id/itv_team_arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12dp"
                android:gravity="center"
                android:textColor="#cbcbcb"
                android:textSize="14dp"
                app:iconCode="eb69" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_alignLeft="@+id/tv_team_group"
                android:layout_below="@+id/tv_team_group"
                android:background="@color/cm_divider_bg" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_team_starttime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="?android:attr/selectableItemBackground">

            <cc.emw.mobile.view.IconTextView
                android:id="@+id/starttime_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:gravity="center"
                android:textColor="#8d8e97"
                android:textSize="16dp"
                app:iconCode="e92c" />

            <TextView
                android:id="@+id/tv_team_starttime"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/starttime_icon"
                android:gravity="center_vertical"
                android:hint="@string/beg_time"
                android:textColor="@color/cm_text"
                android:textColorHint="@color/cm_texthint"
                android:textSize="14sp" />
            <cc.emw.mobile.view.IconTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12dp"
                android:gravity="center"
                android:textColor="#cbcbcb"
                android:textSize="14dp"
                app:iconCode="eb69" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_alignLeft="@+id/tv_team_starttime"
                android:layout_below="@+id/tv_team_starttime"
                android:background="@color/cm_divider_bg" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_team_endtime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="?android:attr/selectableItemBackground">

            <cc.emw.mobile.view.IconTextView
                android:id="@+id/endtime_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:gravity="center"
                android:textColor="#8d8e97"
                android:textSize="16dp"
                app:iconCode="e92c" />

            <TextView
                android:id="@+id/tv_team_endtime"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/endtime_icon"
                android:gravity="center_vertical"
                android:hint="@string/end_time"
                android:textColor="@color/cm_text"
                android:textColorHint="@color/cm_texthint"
                android:textSize="14sp" />
            <cc.emw.mobile.view.IconTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12dp"
                android:gravity="center"
                android:textColor="#cbcbcb"
                android:textSize="14dp"
                app:iconCode="eb69" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_alignLeft="@+id/tv_team_endtime"
                android:layout_below="@+id/tv_team_endtime"
                android:background="@color/cm_divider_bg" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_team_color"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="?android:attr/selectableItemBackground">

            <cc.emw.mobile.view.IconTextView
                android:id="@+id/color_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:gravity="center"
                android:textColor="#8d8e97"
                android:textSize="16dp"
                app:iconCode="eba7" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/color_icon"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/iv_team_color"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_marginRight="6dp"
                    android:src="@color/transparent"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_team_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/trans"
                    android:hint="颜色"
                    android:textColor="@color/cm_text"
                    android:textColorHint="@color/cm_texthint"
                    android:textSize="14sp" />
            </LinearLayout>

            <cc.emw.mobile.view.IconTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12dp"
                android:gravity="center"
                android:textColor="#cbcbcb"
                android:textSize="14dp"
                app:iconCode="eb69" />
        </RelativeLayout>

    </LinearLayout>

    <TextView
        android:id="@+id/tv_team_mainuser"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:paddingLeft="12dp"
        android:textSize="12sp"
        android:text="团队负责人"
        android:visibility="gone"/>
    <cc.emw.mobile.view.ExListView
        android:id="@+id/lv_team_mainuser"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/cm_border_bg"
        android:cacheColorHint="@android:color/transparent"
        android:paddingLeft="12dp"
        android:divider="@color/transparent"
        android:dividerHeight="0dp"
        android:fadingEdge="none"
        android:listSelector="@android:color/transparent" />
    <LinearLayout
        android:id="@+id/ll_team_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:gravity="center_vertical"
        android:visibility="gone">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:text="团队成员"
            />
        <cc.emw.mobile.view.IconTextView
            android:id="@+id/itv_team_adduser"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:textSize="20dp"
            android:textColor="#9298A0"
            app:iconCode="ea5f"/>
    </LinearLayout>
    <cc.emw.mobile.view.ExListView
        android:id="@+id/lv_team_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:background="@drawable/cm_border_bg"
        android:cacheColorHint="@android:color/transparent"
        android:paddingLeft="12dp"
        android:divider="@color/transparent"
        android:dividerHeight="0dp"
        android:fadingEdge="none"
        android:listSelector="@android:color/transparent" />
</LinearLayout>
</ScrollView>
