<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@null"
    android:gravity="center"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <com.brucetoo.imagebrowse.widget.PhotoView
            android:id="@+id/iv_dynamicimage_img1"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:scaleType="centerCrop"
            android:src="@color/gray_1"/>
        <com.brucetoo.imagebrowse.widget.PhotoView
            android:id="@+id/iv_dynamicimage_img2"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginLeft="2dp"
            android:scaleType="centerCrop"
            android:src="@color/gray_1"/>
        <com.brucetoo.imagebrowse.widget.PhotoView
            android:id="@+id/iv_dynamicimage_img3"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginLeft="2dp"
            android:scaleType="centerCrop"
            android:src="@color/gray_1"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:orientation="horizontal">
        <com.brucetoo.imagebrowse.widget.PhotoView
            android:id="@+id/iv_dynamicimage_img4"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:scaleType="centerCrop"
            android:src="@color/gray_1"/>
        <com.brucetoo.imagebrowse.widget.PhotoView
            android:id="@+id/iv_dynamicimage_img5"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginLeft="2dp"
            android:scaleType="centerCrop"
            android:src="@color/gray_1"/>
        <FrameLayout
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginLeft="2dp">
            <com.brucetoo.imagebrowse.widget.PhotoView
                android:id="@+id/iv_dynamicimage_img6"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:scaleType="centerCrop"
                android:src="@color/gray_1"/>
            <TextView
                android:id="@+id/tv_dynamicimage_count"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#B3000000"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="24sp"
                android:text="+9"/>
        </FrameLayout>
    </LinearLayout>
</LinearLayout>