<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/ll_calendar_create_outs"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cm_new_page_bg"
    android:orientation="vertical">

    <include layout="@layout/cm_dialog_bar2" />

    <cc.emw.mobile.view.SwipeBackScrollView
        android:id="@+id/main_calendar_create"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:scrollbars="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@null"
            android:orientation="vertical">

            <EditText
                android:id="@+id/et_phone_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:hint="电话主题"
                android:paddingLeft="16dp"
                android:paddingRight="12dp"
                android:paddingTop="20dp"
                android:singleLine="true"
                android:textColor="@color/cm_text"
                android:textColorHint="@color/cm_texthint"
                android:textSize="16sp"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/et_phone_desc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="top"
                android:hint="电话详情"
                android:paddingBottom="30dp"
                android:paddingLeft="16dp"
                android:paddingRight="12dp"
                android:paddingTop="15dp"
                android:textColor="@color/cm_text"
                android:textColorHint="@color/cm_texthint"
                android:textSize="14sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@color/white"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ll_phone_allday"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="16dp"
                        android:src="@drawable/icon_all_day" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_weight="1"
                        android:text="全天事件"
                        android:textColor="@color/cm_text"
                        android:textSize="14sp" />

                    <cc.emw.mobile.view.SwitchButton
                        android:id="@+id/sb_phone_allday"
                        style="@style/SwitchButtonStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="16dp"
                        android:layout_marginTop="1dp"
                        android:enabled="false"
                        app:kswAnimationDuration="300"
                        app:kswBackDrawable="@drawable/ios_back_drawable"
                        app:kswBackMeasureRatio="1.5"
                        app:kswThumbDrawable="@mipmap/ios_thumb"
                        app:kswThumbMarginBottom="-8dp"
                        app:kswThumbMarginLeft="-5dp"
                        app:kswThumbMarginRight="-5dp"
                        app:kswThumbMarginTop="-2.5dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_phone_startdate"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="16dp"
                        android:textColor="#737373"
                        android:textSize="18dp"
                        app:iconCode="e92c" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_weight="1"
                        android:hint="开始时间"
                        android:textColor="@color/cm_text"
                        android:textColorHint="@color/cm_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/btn_phone_startdate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:textColor="@color/cm_texthint"
                        android:textSize="14sp" />

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="6dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center"
                        android:textColor="#cbcbcb"
                        android:textSize="12dp"
                        app:iconCode="eb69" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_phone_enddate"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="16dp"
                        android:textColor="#737373"
                        android:textSize="18dp"
                        app:iconCode="e92c" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_weight="1"
                        android:hint="结束时间"
                        android:textColor="@color/cm_text"
                        android:textColorHint="@color/cm_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/btn_phone_enddate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:textColor="@color/cm_texthint"
                        android:textSize="14sp" />

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="6dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center"
                        android:textColor="#cbcbcb"
                        android:textSize="12dp"
                        app:iconCode="eb69" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_repeat"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="16dp"
                        android:src="@drawable/icon_repeat" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_weight="1"
                        android:hint="重复"
                        android:textColor="@color/cm_text"
                        android:textColorHint="@color/cm_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_repeat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:textColor="@color/cm_texthint"
                        android:textSize="14sp" />

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="6dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center"
                        android:textColor="#cbcbcb"
                        android:textSize="12dp"
                        app:iconCode="eb69" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_repeat_end"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="16dp"
                        android:textColor="#737373"
                        android:textSize="18dp"
                        app:iconCode="e92c" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_weight="1"
                        android:hint="重复截止"
                        android:textColor="@color/cm_text"
                        android:textColorHint="@color/cm_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_repeat_end"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:textColor="@color/cm_texthint"
                        android:textSize="14sp" />

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="6dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center"
                        android:textColor="#cbcbcb"
                        android:textSize="12dp"
                        app:iconCode="eb69" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_remind"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="16dp"
                        android:textColor="#737373"
                        android:textSize="18dp"
                        app:iconCode="e908" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_weight="1"
                        android:hint="提醒"
                        android:textColor="@color/cm_text"
                        android:textColorHint="@color/cm_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_remind"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:textColor="@color/cm_texthint"
                        android:textSize="14sp" />

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="6dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center"
                        android:textColor="#cbcbcb"
                        android:textSize="12dp"
                        app:iconCode="eb69" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@color/white"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ll_phone_calluser_root"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:gravity="center_vertical">

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="16dp"
                        android:textColor="#737373"
                        android:textSize="18dp"
                        app:iconCode="e985" />

                    <TextView
                        android:id="@+id/tv_phone_calluser"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_weight="1"
                        android:hint="致电人"
                        android:textColor="@color/cm_text"
                        android:textColorHint="@color/cm_text"
                        android:textSize="14sp" />

                    <LinearLayout
                        android:id="@+id/ll_phone_calluser_head"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:orientation="horizontal"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/tv_phone_calluser_num"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginLeft="8dp"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#989898"
                        android:textSize="12sp"
                        android:visibility="gone" />

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="6dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center"
                        android:textColor="#cbcbcb"
                        android:textSize="12dp"
                        app:iconCode="eb69"
                        android:visibility="invisible"/>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_phone_answeruser_root"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:gravity="center_vertical">

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="16dp"
                        android:textColor="#737373"
                        android:textSize="18dp"
                        app:iconCode="e985" />

                    <TextView
                        android:id="@+id/tv_phone_answeruser"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_weight="1"
                        android:hint="接听人"
                        android:textColor="@color/cm_text"
                        android:textColorHint="@color/cm_text"
                        android:textSize="14sp" />

                    <LinearLayout
                        android:id="@+id/ll_phone_answeruser_head"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:orientation="horizontal"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/tv_phone_answeruser_num"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginLeft="8dp"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#989898"
                        android:textSize="12sp"
                        android:visibility="gone" />

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="6dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center"
                        android:textColor="#cbcbcb"
                        android:textSize="12dp"
                        app:iconCode="eb69" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_phone_mainuser_root"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:gravity="center_vertical"
                    android:paddingLeft="16dp"
                    android:paddingRight="10dp">

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:textColor="#737373"
                        android:textSize="18dp"
                        app:iconCode="e912" />

                    <TextView
                        android:id="@+id/tv_phone_mainuser"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_weight="1"
                        android:hint="负责人"
                        android:textColor="@color/cm_text"
                        android:textColorHint="@color/cm_text"
                        android:textSize="14sp" />

                    <LinearLayout
                        android:id="@+id/ll_phone_mainuser_head"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:orientation="horizontal"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/tv_phone_mainuser_num"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginLeft="8dp"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#989898"
                        android:textSize="12sp"
                        android:visibility="gone" />

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="6dp"
                        android:gravity="center"
                        android:textColor="#cbcbcb"
                        android:textSize="12dp"
                        app:iconCode="eb69" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_phone_telnum_root"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="16dp"
                    android:paddingRight="10dp">

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:textColor="#737373"
                        android:textSize="18dp"
                        app:iconCode="ec4c" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:hint="电话号码"
                        android:textColor="@color/cm_text"
                        android:textColorHint="@color/cm_text"
                        android:textSize="14sp" />

                    <EditText
                        android:id="@+id/et_phone_telnum"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="right"
                        android:paddingBottom="5dp"
                        android:paddingTop="5dp"
                        android:singleLine="true"
                        android:textColor="@color/cm_select_text"
                        android:textSize="14sp" />

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="6dp"
                        android:gravity="center"
                        android:textColor="#cbcbcb"
                        android:textSize="12dp"
                        android:visibility="invisible"
                        app:iconCode="eb69" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp">

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:textColor="#737373"
                        android:textSize="18dp"
                        app:iconCode="ea4d" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:paddingBottom="8dp"
                        android:paddingTop="8dp"
                        android:text="电话类型"
                        android:textColor="@color/cm_text"
                        android:textSize="14sp" />

                    <cc.emw.mobile.view.SegmentedGroup xmlns:segmentedgroup="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/segmented_plan_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        segmentedgroup:sc_border_width="1dp"
                        segmentedgroup:sc_checked_text_color="@color/segmentedgroup_checked_text"
                        segmentedgroup:sc_corner_radius="5dp"
                        segmentedgroup:sc_tint_color="@color/segmentedgroup_tint_text">

                        <RadioButton
                            android:id="@+id/rb_plan_day"
                            style="@style/RadioButton"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:checked="true"
                            android:tag="0"
                            android:text="呼出" />

                        <RadioButton
                            android:id="@+id/rb_plan_week"
                            style="@style/RadioButton"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:tag="1"
                            android:text="接听" />
                    </cc.emw.mobile.view.SegmentedGroup>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_phone_emergency_root"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:gravity="center_vertical"
                    android:paddingLeft="16dp"
                    android:paddingRight="10dp">

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:textColor="#737373"
                        android:textSize="18dp"
                        app:iconCode="ea6c" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_weight="1"
                        android:hint="紧急程度"
                        android:textColor="@color/cm_text"
                        android:textColorHint="@color/cm_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_phone_emergency"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:textColor="@color/cm_select_text"
                        android:textColorHint="@color/cm_texthint"
                        android:textSize="14sp" />

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="6dp"
                        android:gravity="center"
                        android:textColor="#cbcbcb"
                        android:textSize="12dp"
                        app:iconCode="eb69" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_phone_file_root"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="18dp"
                    android:paddingRight="10dp">

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:textColor="#737373"
                        android:textSize="18dp"
                        app:iconCode="eb05" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:text="附件"
                        android:textColor="@color/cm_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_phone_file_num"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_weight="1"
                        android:ellipsize="middle"
                        android:gravity="right"
                        android:singleLine="true"
                        android:textColor="@color/cm_select_text"
                        android:textSize="14sp"
                        android:visibility="invisible" />

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="6dp"
                        android:gravity="center"
                        android:textColor="#cbcbcb"
                        android:textSize="12dp"
                        app:iconCode="eb69" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_phone_busdata_root"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="18dp"
                    android:paddingRight="10dp">

                    <cc.emw.mobile.view.IconTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:textColor="#737373"
                        android:textSize="18dp"
                        app:iconCode="eaa8" />

                    <TextView
                        android:id="@+id/tv_phone_busdata"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:text="业务数据"
                        android:textColor="@color/cm_text"
                        android:textSize="14sp" />

                    <LinearLayout
                        android:id="@+id/ll_phone_busdata"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/index_ico_busdata" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_phone_busname"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="业务名称"
                                android:textColor="@color/cm_text"
                                android:textSize="14sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="业务数据"
                                android:textColor="@color/cm_texthint"
