<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#ffffff">
    <TextView
        android:id="@+id/tv_chat_ai_travel_from"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="深圳"/>
    <TextView
        android:id="@+id/tv_chat_ai_travel_to"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:text="北京"/>
    <TextView
        android:id="@+id/tv_chat_ai_travel_from_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_chat_ai_travel_from"
        android:text="08:24"/>
    <TextView
        android:id="@+id/tv_chat_ai_travel_to_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/tv_chat_ai_travel_to"
        android:text="08:24"/>

</RelativeLayout>