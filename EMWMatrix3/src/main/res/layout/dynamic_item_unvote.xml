<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="32dp"
    android:background="@drawable/vote_bg"
    android:orientation="horizontal"
    android:gravity="center"
    android:paddingLeft="12dp"
    android:paddingRight="12dp">
    <CheckBox
        android:id="@+id/cb_dynamicvote_select"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:button="@drawable/cm_radio_select1"
        android:clickable="false"
        android:focusable="false"
        android:visibility="gone"/>
    <TextView
        android:id="@+id/tv_dynamicvote_position"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1."
        android:textColor="@color/dynamicvote_name_text"
        android:textSize="14sp"
        android:visibility="gone"/>
    <View
        android:id="@+id/view_dynamicvote_temp"
        android:layout_width="32dp"
        android:layout_height="32dp"
        />
    <TextView
        android:id="@+id/tv_dynamicvote_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp"
        android:gravity="center"
        android:maxLines="1"
        android:ellipsize="end"
        android:textColor="@color/dynamicvote_name_text"
        android:textSize="12sp" />
    <TextView
        android:id="@+id/tv_dynamicvote_percent"
        android:layout_width="32dp"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:maxLines="1"
        android:textColor="@color/dynamicvote_percent_text"
        android:textSize="12sp"
        android:text="100%" />
    <!--<View
        android:layout_width="20dp"
        android:layout_height="20dp"
        />-->
</LinearLayout>